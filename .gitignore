# IDE files (Eclipse, IntelliJ, etc.)
.idea/              # IntelliJ IDEA project files
*.iml              # IntelliJ IDEA module files
.classpath         # Eclipse classpath file
.project           # Eclipse project file
.settings/         # Eclipse settings folder

# Compiled output (Maven)
target/            # Maven build output directory (contains .class files, .jar, etc.)
*.class            # Compiled Java class files

# Logs
*.log              # Log files generated by the application

# Dependency directories
.m2/               # Maven local repository cache

# OS-generated files
.DS_Store          # macOS Finder metadata files
Thumbs.db          # Windows thumbnail cache files